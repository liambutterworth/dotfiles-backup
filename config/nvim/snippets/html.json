{
    "doctype": {
        "prefix": "doctype",
        "description": "Defines the document type",

        "body": [
            "<!DOCTYPE>",
            "$1"
        ]
    },

    "a": {
        "prefix": "a",
        "description": "Defines a hyperlink",
        "body": "<a href=\"$1\">${2:$TM_SELECTED_TEXT}</a>"
    },

    "address": {
        "prefix": ["add", "address"],
        "description": "Defines an address element",

        "body": [
            "<address>",
            "\t$TM_SELECTED_TEXT$1",
            "</address>"
        ]
    },

    "article": {
        "prefix": "article",
        "description": "Defines an article",

        "body": [
            "<article>",
            "\t$TM_SELECTED_TEXT$1",
            "</article>"
        ]
    },

    "aside": {
        "prefix": "aside",
        "description": "Defines content aside from the page content",

        "body": [
            "<aside>",
            "\t$TM_SELECTED_TEXT$1",
            "</aside>"
        ]
    },

    "b": {
        "prefix": "b",
        "description": "Defines bold text",
        "body": "<b>${1:$TM_SELECTED_TEXT}</b>"
    },

    "body": {
        "prefix": "body",
        "description": "Defines the body element",

        "body": [
            "<body>",
            "\t$TM_SELECTED_TEXT$1",
            "</body>"
        ]
    },

    "button": {
        "prefix": "button",
        "description": "Defines a push button",
        "body": "<button type=\"$1\">${2:$TM_SELECTED_TEXT}</button>"
    },

    "div": {
        "prefix": "div",
        "description": "Defines a section in a document",

        "body": [
            "<div>",
            "\t$TM_SELECTED_TEXT$1",
            "</div>"
        ]
    },

    "em": {
        "prefix": "em",
        "description": "Defines emphasized text",
        "body": "<em>${1:$TM_SELECTED_TEXT}</em>"
    },

    "fieldset": {
        "prefix": "fieldset",
        "description": "Defines a fieldset",

        "body": [
            "<fieldset>",
            "\t$TM_SELECTED_TEXT$1",
            "</fieldset>"
        ]
    },

    "footer": {
        "prefix": "footer",
        "description": "Defines a footer for a section or page",

        "body": [
            "<footer>",
            "\t$TM_SELECTED_TEXT$1",
            "</footer>"
        ]
    },

    "form": {
        "prefix": "form",
        "description": "Defines a form",

        "body": [
            "<form>",
            "\t$TM_SELECTED_TEXT$1",
            "</form>"
        ]
    },

    "h1": {
        "prefix": "h1",
        "description": "Defines header 1",
        "body": "<h1>${1:$TM_SELECTED_TEXT}</h1>"
    },

    "h2": {
        "prefix": "h2",
        "description": "Defines header 2",
        "body": "<h2>${1:$TM_SELECTED_TEXT}</h2>"
    },

    "h3": {
        "prefix": "h3",
        "description": "Defines header 3",
        "body": "<h3>${1:$TM_SELECTED_TEXT}</h3>"
    },

    "h4": {
        "prefix": "h4",
        "description": "Defines header 4",
        "body": "<h4>${1:$TM_SELECTED_TEXT}</h4>"
    },

    "h5": {
        "prefix": "h5",
        "description": "Defines header 5",
        "body": "<h5>${1:$TM_SELECTED_TEXT}</h5>"
    },

    "h6": {
        "prefix": "h6",
        "description": "Defines header 6",
        "body": "<h6>${1:$TM_SELECTED_TEXT}</h6>"
    },

    "head": {
        "prefix": "head",
        "description": "Defines information about the document",

        "body": [
            "<head>",
            "\t$TM_SELECTED_TEXT$1",
            "</head>"
        ]
    },

    "header": {
        "prefix": "header",
        "description": "Defines a header for a section of page",

        "body": [
            "<header>",
            "\t$TM_SELECTED_TEXT$1",
            "</header>"
        ]
    },

    "hgroup": {
        "prefix": "hgroup",
        "description": "Defines information about a section in a document",

        "body": [
            "<hgroup>",
            "\t$TM_SELECTED_TEXT$1",
            "</hgroup>"
        ]
    },

    "hr": {
        "prefix": "hr",
        "description": "Defines a horizontal rule",
        "body": "<hr />"
    },

    "html": {
        "prefix": "html",
        "description": "Defines an html document",

        "body": [
            "<html>",
            "\t$TM_SELECTED_TEXT$1",
            "</html>"
        ]
    },

    "html5": {
        "prefix": "html5",
        "description": "Defines a template for a html5 document",

        "body": [
            "<!DOCTYPE html>",
            "",
            "<html lang=\"${1:en}\">",
            "\t<head>",
            "\t\t<title>${2:Title}</title>",
            "\t\t<meta charset=\"UTF-8\">",
            "\t\t<meta name=\"viewport\" content=\"width=device-width, initial-scale=1\">",
            "\t</head>",
            "",
            "\t<body>",
            "\t\t$TM_SELECTED_TEXT$3",
            "\t</body>",
            "</html>"
        ]
    },

    "i": {
        "prefix": "i",
        "description": "Defines italic text",
        "body": "<i>${1:$TM_SELECTED_TEXT}</i>"
    },

    "img": {
        "prefix": "img",
        "description": "Defines an image",
        "body": "<img src=\"$1\" alt=\"$2\" />"
    },

    "input": {
        "prefix": "input",
        "description": "Defines an input field",
        "body": "<input type=\"$1\" name=\"$2\" value=\"$3\" />"
    },

    "label": {
        "prefix": "label",
        "description": "Defines an inline window",
        "body": "<label for=\"$1\">${1:$TM_SELECTED_TEXT}</label>"
    },

    "legend": {
        "prefix": "legend",
        "description": "Defines a title in a fieldset",
        "body": "<legend>${1:$TM_SELECTED_TEXT}</legend>"
    },

    "li": {
        "prefix": "li",
        "description": "Defines a list item",
        "body": "<li>${1:$TM_SELECTED_TEXT}</li>"
    },

    "link": {
        "prefix": "link",
        "description": "Defines a resource reference",
        "body": "<link rel=\"$1\" type=\"$2\" href=\"$3\" />"
    },

    "main": {
        "prefix": "main",
        "description": "Defines an image map",

        "body": [
            "<main>",
            "\t$TM_SELECTED_TEXT$1",
            "</main>"
        ]
    },

    "meta": {
        "prefix": "meta",
        "body": "<meta name=\"$1\" content=\"$2\">",
        "description": "Defines meta information"
    },

    "nav": {
        "prefix": "nav",
        "description": "Defines navigation links",

        "body": [
            "<nav>",
            "\t$TM_SELECTED_TEXT$1",
            "</,nav>"
        ]
    },

    "ol": {
        "prefix": "ol",
        "description": "Defines an ordered list",

        "body": [
            "<ol>",
            "\t$TM_SELECTED_TEXT$1",
            "</ol>"
        ]
    },

    "optgroup": {
        "prefix": "optgroup",
        "description": "Defines an option group",

        "body": [
            "<optgroup>",
            "\t$1",
            "</optgroup>"
        ]
    },

    "option": {
        "prefix": "option",
        "description": "Defines an option in a drop-down list",
        "body": "<option value=\"$1\">${2:$TM_SELECTED_TEXT}</option>"
    },

    "p": {
        "prefix": "p",
        "description": "Defines a paragraph",
        "body": "<p>${1:$TM_SELECTED_TEXT}</p>$2"
    },

    "pre": {
        "prefix": "pre",
        "description": "Defines preformatted text",
        "body": "<pre>$1</pre>"
    },

    "q": {
        "prefix": "q",
        "description": "Defines a short quotation",
        "body": "<q>${1:$TM_SELECTED_TEXT}</q>"
    },

    "s": {
        "prefix": "s",
        "description": "Used to define strikethrough text",
        "body": "<s>${1:$TM_SELECTED_TEXT}</s>"
    },

    "script": {
        "prefix": "script",
        "description": "Defines a script",

        "body": [
            "<script>",
            "\t$TM_SELECTED_TEXT$1",
            "</script>"
        ]
    },

    "section": {
        "prefix": "section",
        "description": "Defines a section",

        "body": [
            "<section>",
            "\t$TM_SELECTED_TEXT$1",
            "</section>"
        ]
    },

    "select": {
        "prefix": "select",
        "description": "Defines a selectable list",

        "body": [
            "<select>",
            "\t$TM_SELECTED_TEXT$1",
            "</select>"
        ]
    },

    "small": {
        "prefix": "small",
        "description": "Defines small text",
        "body": "<small>${1:$TM_SELECTED_TEXT}</small>"
    },

    "span": {
        "prefix": "span",
        "body": "<span>${1:$TM_SELECTED_TEXT}</span>",
        "description": "Defines a section in a document"
    },

    "strong": {
        "prefix": "strong",
        "description": "Defines strong text",
        "body": "<strong>${1:$TM_SELECTED_TEXT}</strong>"
    },

    "style": {
        "prefix": "style",
        "description": "Defines a style definition",

        "body": [
            "<style>",
            "",
            "$1",
            "",
            "</style>"
        ]
    },

    "sub": {
        "prefix": "sub",
        "description": "Defines sub-scripted text",
        "body": "<sub>${1:$TM_SELECTED_TEXT}</sub>"
    },

    "sup": {
        "prefix": "sup",
        "description": "Defines super-scripted text",
        "body": "<sup>${1:$TM_SELECTED_TEXT}</sup>"
    },

    "table": {
        "prefix": "table",
        "description": "Defines a table",

        "body": [
            "<table>",
            "\t${1:$TM_SELECTED_TEXT}",
            "</table>"
        ]
    },

    "tbody": {
        "prefix": "tbody",
        "description": "Defines a table body",

        "body": [
            "<tbody>",
            "\t$TM_SELECTED_TEXT$1",
            "</tbody>"
        ]
    },

    "td": {
        "prefix": "td",
        "body": "<td>${1:$TM_SELECTED_TEXT}</td>$2",
        "description": "Defines a table cell"
    },

    "textarea": {
        "prefix": "textarea",
        "description": "Defines a text area",
        "body": "<textarea rows=\"$1\" cols=\"$2\">${3:$TM_SELECTED_TEXT}</textarea>"
    },

    "tfoot": {
        "prefix": "tfoot",
        "description": "Defines a table footer",

        "body": [
            "<tfoot>",
            "\t$TM_SELECTED_TEXT$1",
            "</tfoot>"
        ]
    },

    "thead": {
        "prefix": "thead",
        "description": "Defines a table head",

        "body": [
            "<thead>",
            "\t$TM_SELECTED_TEXT$1",
            "</thead>"
        ]
    },

    "th": {
        "prefix": "th",
        "description": "Defines a table header",
        "body": "<th>${1:$TM_SELECTED_TEXT}</th>"
    },

    "title": {
        "prefix": "title",
        "body": "<title>$1</title>$2",
        "description": "Defines the document title"
    },

    "tr": {
        "prefix": "tr",
        "description": "Defines a table row",

        "body": [
            "<tr>",
            "\t$TM_SELECTED_TEXT$1",
            "</tr>"
        ]
    },

    "u": {
        "prefix": "u",
        "description": "Used to define underlined text",
        "body": "<u>$TM_SELECTED_TEXT$1</u>"
    },

    "ul": {
        "prefix": "ul",
        "description": "Defines an unordered list",

        "body": [
            "<ul>",
            "\t$TM_SELECTED_TEXT$1",
            "</ul>"
        ]
    }
}
